{
    "manifest_version": 2,
    "name": "Theatre",
    "version": "1.0",
    "description": "To let chrome become a theatre",
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "web_accessible_resources": [
        "views/*.html",
        "views/*.css",
        "imgs/*"
    ]
}
